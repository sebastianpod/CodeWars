    "def stringy(size):\n",
    "    strng = ''\n",
    "    for i in range(size):\n",
    "        if i % 2 == 0:\n",
    "            strng += '1'\n",
    "        else:\n",
    "            strng += '0'\n",
    "    return strng\n",
    "\n",
    "stringy(12)"
   ]
