   "source": [
    "def next_item(xs, item):\n",
    "    it = iter(xs)\n",
    "    for x in it:\n",
    "        if x == item:\n",
    "            break\n",
    "    return next(it, None)\n",
    " \n",
    "next_item(['a', 'b', 'c', 'd', 'e', 'f'], 'a')"
   ]
