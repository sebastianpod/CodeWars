    "def points(games):\n",
    "    x = 0\n",
    "    for i in games:\n",
    "        if i[0] > i[2]:\n",
    "            x += 3\n",
    "        elif i[0] == i[2]:\n",
    "            x += 1\n",
    "    return x           "
