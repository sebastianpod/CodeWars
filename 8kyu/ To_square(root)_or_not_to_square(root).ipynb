    "import math\n",
    "def square_or_square_root(arr):\n",
    "    lista = []\n",
    "    for i in arr:\n",
    "        if math.sqrt(i) % 1 == 0:\n",
    "            lista.append(math.sqrt(i))\n",
    "        else:\n",
    "            lista.append(i ** 2)\n",
    "    return lista\n",
    "square_or_square_root([4,3,9,7,2,1])"
   ]
