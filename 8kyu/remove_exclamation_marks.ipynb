   "source": [
    "def remove_exclamation_marks(s):\n",
    "    strng = ''\n",
    "    for char in s:\n",
    "        if char == '!':\n",
    "            strng += ''\n",
    "        else:\n",
    "            strng += char\n",
    "    return strng"
   ]
