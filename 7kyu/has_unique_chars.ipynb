    "def has_unique_chars(string):\n",
    "    if string == '':\n",
    "        return False\n",
    "    else:\n",
    "        x = [i for i in string.replace(' ','')]\n",
    "        x = dict.fromkeys(x)\n",
    "        return True if ''.join(x) == string.replace(' ','') else False\n",
    "has_unique_chars('')"
   ]
